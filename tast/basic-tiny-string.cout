## tast_operator
-- new tiny string in heap
|| p32->capacity() =~? 31
|| p32->size() =~? 4
|| p32->length() =~? 4
1234
-- push_back(c)
|| p32->size() =~? 5
1234$
-- operator += char or string, based append
|| p32->size() =~? 10
1234$$4321
-- operator << chains
|| p32->size() =~? 16
1234$$4321#@abcd
-- define tiny string in stack
|| s32.size() =~? 4
abcd
-- default operator= for tiny string, memory copy
|| s32.size() =~? 16
1234$$4321#@abcd
|| s32 == *p32 =~? 1
|| p32->size() =~? 0
|| p32->empty() =~? 1

